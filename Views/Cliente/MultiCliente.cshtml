@model List<ClienteModel>

@{
    ViewData["Title"] = "MultiCliente";
}
<h1>DATOS DEL CLIENTE</h1>

<table class="table table-white table-hover">
    <thead>
        <tr>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                NOMBRE
            </th>
            <th scope="col">
                APELLIDO
            </th>
            <th scope="col">
                CORREO
            </th>
            <th scope="col">
                CLAVE
            </th>
            <th scope="col">
                FECHA DE CREACION
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cliente in Model)
        {
            <tr>

                <td>
                    @cliente.Id
                </td>
                <td>
                    @cliente.Nombre
                </td>
                <td>
                    @cliente.Apellido
                </td>
                <td>
                    @cliente.Correo
                </td>
                <td>
                    @cliente.Clave
                </td>
                <td>
                    @cliente.FechaCreacion
                </td>
            </tr>
        }
    </tbody>

</table>


