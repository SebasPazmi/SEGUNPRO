@model ClienteModel
@{
    ViewData["Title"] = "Crear Cliente";
    Layout = "Simple";
}
@*<div asp-validation-summary="All"></div>*@
<div>
    <form method="POST">
        <label asp-for="Id"></label>
        <input asp-for="Id" />
        <br />
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" />

        <br />
        <label asp-for="Apellido"></label>
        <input asp-for="Apellido" />
        <br />
        <label asp-for="Correo"></label>
        <input asp-for="Correo" />
        <br />
        <label asp-for="Clave"></label>
        <input asp-for="Clave" />
        <br />
        <label asp-for="FechaCreacion"></label>
        <input asp-for="FechaCreacion" />
        <br />
        <input type="submit" />
    </form>
</div>