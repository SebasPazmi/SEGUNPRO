@model List<ProveedorModel>

@{
    ViewData["Title"] = "MultiProveedor";
}
<h1>DATOS DEL PROVEEDOR</h1>
<table class="table" cellpadding="0" cellspacing="0" width="75%" bgcolor="#84b6f4">
    <thead>
        <tr>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                NOMBRE
            </th>
            <th scope="col">
                TELEFONO
            </th>
            <th scope="col">
                CORREO
            </th>
            <th scope="col">
                DIRECCION
            </th>
            <th scope="col">
                RUC
            </th>
            <th scope="col">
                IDMEDICAMENTO
            </th>
            <th scope="col">
                CLAVE
            </th>
            <th scope="col">
                FECHACREACION
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var proveedor in Model)
        {
            <tr>
                <td>
                    @proveedor.Id
                </td>
                <td>
                    @proveedor.Nombre
                </td>
                <td>
                    @proveedor.Telefono
                </td>
                <td>
                    @proveedor.Correo
                </td>
                <td>
                    @proveedor.Direccion
                </td>
                <td>
                    @proveedor.Ruc
                </td>
                <td>
                    @proveedor.IdMedicamento
                </td>
                <td>
                    @proveedor.Clave
                </td>
                <td>
                    @proveedor.FechaCreacion
                </td>
            </tr>
        }
    </tbody>
</table>