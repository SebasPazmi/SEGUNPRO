
@model EmpleadoModel
@{
    ViewData["Title"] = "Crear Empleado";
    Layout = "Simple";
}
@*<div asp-validation-summary="All"></div>*@
<div>
    <form method="POST">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" />
        <br />
        <label asp-for="Apellido"></label>
        <input asp-for="Apellido" />
        
        <br />
        <label asp-for="Direccion"></label>
        <input asp-for="Direccion" />
        <br />
        <label asp-for="Telefono"></label>
        <input asp-for="Telefono" />
        <br />
        <label asp-for="Correo"></label>
        <input asp-for="Correo" />
        <br />
        <label asp-for="Cedula"></label>
        <input asp-for="Cedula" />
        <br />
        <label asp-for="FechaNacimiento"></label>
        <input asp-for="FechaNacimiento" />
        <br />
        <label asp-for="IdUsuario"></label>
        <input asp-for="IdUsuario" />
        <br />
        <label asp-for="Clave"></label>
        <input asp-for="Clave" />
        <br />
        <label asp-for="FechaCreacion"></label>
        <input asp-for="FechaCreacion" />
        <br />
        <input type="submit" />
    </form>
</div>
