@model List<EmpleadoModel>

@{
    ViewData["Title"] = "MultiEmpleado";
}
<h1>DATOS DEL EMPLEADO</h1>
<table class="table" cellpadding="0" cellspacing="0" width="75%" bgcolor="#84b6f4">
    <thead>
        <tr>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                NOMBRE
            </th>
            <th scope="col">
                APELLIDO
            </th>
            <th scope="col">
                DIRECCION
            </th>
            <th scope="col">
                TELEFONO
            </th>
            <th scope="col">
                CORREO
            </th>
            <th scope="col">
                CEDULA
            </th>
            <th scope="col">
                FECHANACIMIENTO
            </th>
            <th scope="col">
                IDUSUARIO
            </th>
            <th scope="col">
                CLAVE
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var empleado in Model)
        {
            <tr>
                <td>
                    @empleado.Id
                </td>
                <td>
                    @empleado.Nombre
                </td>
                <td>
                    @empleado.Apellido
                </td>
                <td>
                    @empleado.Direccion
                </td>
                <td>
                    @empleado.Telefono
                </td>
                <td>
                    @empleado.Correo
                </td>
                <td>
                    @empleado.Cedula
                </td>
                <td>
                    @empleado.FechaNacimiento
                </td>
                <td>
                    @empleado.IdUsuario
                </td>
                <td>
                    @empleado.Clave
                </td>
              <td>
                    @empleado.FechaCreacion
           </td>
      
            </tr>
        }
    </tbody>
</table>